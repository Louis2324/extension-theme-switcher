{
  "manifest_version": 3,
  "name": "Theme Switcher",
  "version": "1.0",
  "permissions": ["activeTab"],
  "action": {
    "default_popup": "popup/popup.html",
    "default_title": "Click to change themes"
  },
  "content_scripts": [
    {
      "matches": [
        "https://example.com/*",
        "http://example.com/*",
        "https://www.example.com/*"
      ],
      "js": ["content/content.js"]
    }
  ]
}
